PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX yago: <http://yago-knowledge.org/resource/>

start = @<Continent>

<Continent> EXTRA rdf:type {
  rdf:type [ schema:Continent ] ;
  rdfs:comment rdf:langString + ;
  rdfs:label rdf:langString + ;
  schema:alternateName rdf:langString + ;
  schema:image xsd:anyURI * ;
  schema:mainEntityOfPage xsd:anyURI + ;
  schema:sameAs xsd:anyURI + ;
  schema:url xsd:anyURI ? ;
  schema:geo geo:wktLiteral ? ;
  schema:location @<Place> * ;
  yago:area xsd:decimal ? ;
  yago:highestPoint @<Place> ? ;
  yago:lowestPoint @<Place> ? ;
  yago:neighbors @<Place> * ;
  yago:demonym xsd:string * ;
  yago:populationNumber xsd:decimal * ;
  owl:sameAs [ <http://www.wikidata.org/entity>~ ] *
}

<Place> EXTRA rdf:type {
  rdf:type [ schema:Place ]
}
