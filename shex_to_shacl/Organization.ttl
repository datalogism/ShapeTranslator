@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix yago: <http://yago-knowledge.org/resource/> .

<http://shaclshapes.org/OrganizationShape> a sh:NodeShape ;
    sh:property [ sh:datatype xsd:string ;
            sh:path schema:address ],
        [ sh:datatype xsd:anyURI ;
            sh:minCount 1 ;
            sh:path schema:mainEntityOfPage ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:url ],
        [ sh:datatype xsd:string ;
            sh:path schema:slogan ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path owl:sameAs ;
            sh:pattern "^http://www.wikidata.org/entity/" ],
        [ sh:datatype rdf:langString ;
            sh:path schema:alternateName ],
        [ sh:class schema:Place ;
            sh:path schema:locationCreated ],
        [ sh:datatype rdf:langString ;
            sh:path rdfs:comment ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:sameAs ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:path schema:duns ],
        [ sh:class schema:Organization ;
            sh:path schema:memberOf ],
        [ sh:class schema:Person ;
            sh:path yago:leader ],
        [ sh:datatype xsd:string ;
            sh:path schema:motto ],
        [ sh:class [ sh:or ( schema:Organization schema:Person ) ] ;
            sh:path yago:ownedBy ],
        [ sh:class schema:Place ;
            sh:path schema:location ],
        [ sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:path schema:dissolutionDate ],
        [ sh:datatype xsd:decimal ;
            sh:path schema:numberOfEmployees ],
        [ sh:class yago:Award ;
            sh:path schema:award ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:path schema:leiCode ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:logo ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:image ],
        [ sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:path schema:dateCreated ],
        [ sh:datatype rdf:langString ;
            sh:minCount 1 ;
            sh:path rdfs:label ],
        [ sh:class schema:Person ;
            sh:path schema:founder ] ;
    sh:targetClass schema:Organization .

