PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX yago: <http://yago-knowledge.org/resource/>

start = @<Person>

<Person> EXTRA rdf:type {
  rdf:type [ schema:Person ] ;
  rdfs:label xsd:string + ;
  rdfs:comment xsd:string * ;
  schema:alternateName xsd:string * ;
  schema:image xsd:anyURI * ;
  schema:mainEntityOfPage xsd:anyURI + ;
  schema:sameAs xsd:anyURI * ;
  schema:url xsd:anyURI * ;
  schema:affiliation @<Organization> * ;
  schema:alumniOf @<Organization> * ;
  schema:award @<Award> * ;
  schema:birthDate xsd:dateTime ? ;
  schema:birthPlace @<Place> ? ;
  schema:children @<Person> * ;
  schema:deathDate xsd:dateTime ? ;
  schema:deathPlace @<Place> ? ;
  schema:gender @<Gender> ? ;
  schema:homeLocation @<Place> * ;
  schema:knowsLanguage @<Language> * ;
  schema:memberOf @<Organization> * ;
  schema:nationality @<Country> * ;
  schema:owns IRI * ;
  schema:spouse @<Person> * ;
  schema:worksFor @<Organization> * ;
  yago:academicDegree @<Academic_degree> * ;
  yago:beliefSystem @<BeliefSystem> * ;
  yago:appearsIn @<CreativeWork> * ;
  owl:sameAs [ <http://www.wikidata.org/entity>~ ] *
}

<Academic_degree> EXTRA rdf:type {
  rdf:type [ yago:Academic_degree ]
}

<Award> EXTRA rdf:type {
  rdf:type [ yago:Award ]
}

<BeliefSystem> EXTRA rdf:type {
  rdf:type [ yago:BeliefSystem ]
}

<Country> EXTRA rdf:type {
  rdf:type [ schema:Country ]
}

<CreativeWork> EXTRA rdf:type {
  rdf:type [ schema:CreativeWork ]
}

<Gender> EXTRA rdf:type {
  rdf:type [ yago:Gender ]
}

<Language> EXTRA rdf:type {
  rdf:type [ schema:Language ]
}

<Organization> EXTRA rdf:type {
  rdf:type [ schema:Organization ]
}

<Place> EXTRA rdf:type {
  rdf:type [ schema:Place ]
}
