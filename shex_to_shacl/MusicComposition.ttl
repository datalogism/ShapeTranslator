@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://shaclshapes.org/MusicCompositionShape> a sh:NodeShape ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path owl:sameAs ;
            sh:pattern "^http://www.wikidata.org/entity/" ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:path schema:iswcCode ],
        [ sh:class [ sh:or ( schema:Person schema:MusicGroup ) ] ;
            sh:path schema:lyricist ],
        [ sh:datatype rdf:langString ;
            sh:minCount 1 ;
            sh:path rdfs:label ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:sameAs ],
        [ sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:path schema:dateCreated ],
        [ sh:class [ sh:or ( schema:Person schema:MusicGroup ) ] ;
            sh:path schema:musicBy ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:image ],
        [ sh:class [ sh:or ( schema:Person schema:Organization ) ] ;
            sh:path schema:author ],
        [ sh:datatype rdf:langString ;
            sh:maxCount 1 ;
            sh:path schema:alternateName ],
        [ sh:class schema:Person ;
            sh:path schema:editor ],
        [ sh:datatype rdf:langString ;
            sh:path rdfs:comment ],
        [ sh:datatype xsd:anyURI ;
            sh:minCount 1 ;
            sh:path schema:mainEntityOfPage ],
        [ sh:class schema:Language ;
            sh:path schema:inLanguage ] ;
    sh:targetClass schema:MusicComposition .

