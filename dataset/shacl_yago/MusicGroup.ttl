@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix yago: <http://yago-knowledge.org/resource/> .

#################################################################
# MusicGroup Shape
#################################################################
<http://shaclshapes.org/MusicGroupShape>
    a sh:NodeShape ;
    sh:targetClass schema:MusicGroup ;
    ############################## schema:Thing property
    sh:property [
        sh:path rdfs:label ;
        sh:datatype rdf:langString ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:langString ;
    ] ;
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
    ] ;
    sh:property [
        sh:path schema:image ;
        sh:datatype xsd:anyURI ;
    ] ;
    sh:property [
        sh:path schema:mainEntityOfPage ;
        sh:datatype xsd:anyURI ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path schema:sameAs ;
        sh:datatype xsd:anyURI ;
    ] ;
    sh:property [
        sh:path schema:url ;
        sh:datatype xsd:anyURI ;
    ] ;

    ############################## schema.org property
    sh:property [
        sh:path schema:recordLabel ;
        sh:class schema:Organization ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path yago:Human_Language_Q20162172 ;
        sh:class schema:Language ;
    ] ;
    sh:property [
        sh:path schema:award ;
        sh:class yago:Award ;
    ] ;
    sh:property [
        sh:path schema:dateCreated ;
        sh:datatype xsd:dateTime ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:dissolutionDate ;
        sh:datatype xsd:dateTime ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:founder ;
        sh:class schema:Person ;
    ] ;
    sh:property [
        sh:path schema:location ;
        sh:class schema:Place ;
    ] ;
    sh:property [
        sh:path schema:locationCreated ;
        sh:class schema:Place ;
    ] ;
    sh:property [
        sh:path schema:logo ;
        sh:datatype xsd:anyURI ;
        sh:maxCount 1 ;
    ] ;


    ############################## Yago property
    # yago:influencedBy ?
    sh:property [
        sh:path yago:influencedBy ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path yago:director ;
        sh:class  schema:Person ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path yago:notableWork ;
        sh:class schema:CreativeWork ;
    ] ;
    sh:property [
        sh:path yago:ownedBy ;
        sh:class [ sh:or ( schema:Organization schema:Person ) ] ;
    ] ;

    ############################## owl:sameAs restricted to Wikidata
    sh:property [
        sh:path owl:sameAs ;
        sh:pattern "^http://www.wikidata.org/entity/" ;
    ] .