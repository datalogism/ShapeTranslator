@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix yago: <http://yago-knowledge.org/resource/> .

<http://shaclshapes.org/AstronomicalObjectShape> a sh:NodeShape ;
    sh:property [ sh:datatype xsd:anyURI ;
            sh:minCount 1 ;
            sh:path schema:mainEntityOfPage ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path owl:sameAs ;
            sh:pattern "^http://www.wikidata.org/entity/" ],
        [ sh:datatype rdf:langString ;
            sh:minCount 1 ;
            sh:path rdfs:label ],
        [ sh:datatype rdf:langString ;
            sh:minCount 1 ;
            sh:path rdfs:comment ],
        [ sh:datatype xsd:anyURI ;
            sh:maxCount 1 ;
            sh:path schema:image ],
        [ sh:datatype xsd:decimal ;
            sh:path yago:distanceFromEarth ],
        [ sh:datatype xsd:decimal ;
            sh:path yago:mass ],
        [ sh:class yago:AstronomicalObject ;
            sh:path yago:parentBody ],
        [ sh:datatype xsd:decimal ;
            sh:path yago:parallax ],
        [ sh:datatype xsd:decimal ;
            sh:path yago:radialVelocity ],
        [ sh:datatype xsd:decimal ;
            sh:path yago:luminosity ],
        [ sh:datatype xsd:anyURI ;
            sh:maxCount 1 ;
            sh:path schema:sameAs ],
        [ sh:datatype rdf:langString ;
            sh:path schema:alternateName ] ;
    sh:targetClass yago:AstronomicalObject .

