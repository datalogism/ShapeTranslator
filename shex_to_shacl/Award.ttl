@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix yago: <http://yago-knowledge.org/resource/> .

<http://shaclshapes.org/AwardShape> a sh:NodeShape ;
    sh:property [ sh:datatype rdf:langString ;
            sh:path schema:alternateName ],
        [ sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path schema:about ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:url ],
        [ sh:datatype rdf:langString ;
            sh:path rdfs:comment ],
        [ sh:datatype xsd:string ;
            sh:path schema:sameAs ],
        [ sh:datatype xsd:anyURI ;
            sh:path schema:image ],
        [ sh:datatype rdf:langString ;
            sh:minCount 1 ;
            sh:path rdfs:label ],
        [ sh:datatype xsd:anyURI ;
            sh:minCount 1 ;
            sh:path schema:mainEntityOfPage ],
        [ sh:class [ sh:or ( schema:Organization schema:Person ) ] ;
            sh:path yago:conferredBy ],
        [ sh:class schema:Language ;
            sh:path schema:inLanguage ],
        [ sh:class schema:Place ;
            sh:maxCount 1 ;
            sh:path schema:locationCreated ],
        [ sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:path schema:dateCreated ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path owl:sameAs ;
            sh:pattern "^http://www.wikidata.org/entity/" ] ;
    sh:targetClass yago:Award .

